using Oxide.Ext.Chaos.UIFramework;

public class StylePreset
{
	public Style Background;

	public Style Button;

	public Style Dropdown;

	public Style DropdownBody;

	public Style DropdownItem;

	public Style DropdownToggle;

	public Style DisabledButton;

	public Style Header;

	public Style Panel;

	public Style Toggle;

	public Style Title;

	public Style Close;

	public StylePreset()
	{
		Background = ChaosStyle.Background;
		Button = ChaosStyle.Button;
		Dropdown = ChaosStyle.DropdownButton;
		DropdownBody = ChaosStyle.DropdownBody;
		DropdownItem = ChaosStyle.DropdownItem;
		DropdownToggle = ChaosStyle.Toggle;
		DisabledButton = ChaosStyle.DisabledButton;
		Header = ChaosStyle.Header;
		Panel = ChaosStyle.Panel;
		Toggle = ChaosStyle.Toggle;
		Title = ChaosStyle.Title;
		Close = ChaosStyle.Close;
	}
}
