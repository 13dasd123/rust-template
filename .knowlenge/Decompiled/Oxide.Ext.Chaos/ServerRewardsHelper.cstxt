public class ServerRewardsHelper : BasePluginHelper
{
	public ServerRewardsHelper()
		: base("ServerRewards", "Server Rewards")
	{
	}

	public object AddPoints(object userID, int amount)
	{
		return Call<object>("AddPoints", new object[2] { userID, amount });
	}

	public object TakePoints(object userID, int amount)
	{
		return Call<object>("TakePoints", new object[2] { userID, amount });
	}

	public int CheckPoints(object userID)
	{
		return Call<int>("CheckPoints", new object[1] { userID });
	}

	public object GetUserID(object userID)
	{
		return Call<object>("GetUserID", new object[1] { userID });
	}
}
