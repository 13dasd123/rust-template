public class PlaytimeTrackerHelper : BasePluginHelper
{
	public PlaytimeTrackerHelper()
		: base("PlaytimeTracker", "Playtime Tracker")
	{
	}

	public object GetPlayTime(string id)
	{
		return Call<object>("GetPlayTime", new object[1] { id });
	}

	public object GetAFKTime(string id)
	{
		return Call<object>("GetAFKTime", new object[1] { id });
	}

	public object GetReferrals(string id)
	{
		return Call<object>("GetReferrals", new object[1] { id });
	}
}
