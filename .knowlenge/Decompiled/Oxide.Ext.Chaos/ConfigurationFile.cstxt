using Oxide.Core.Configuration;

protected abstract class ConfigurationFile
{
	protected DynamicConfigFile Config;

	public abstract BaseConfigData Data { get; set; }

	public ConfigurationFile(DynamicConfigFile dynamicConfigFile)
	{
		Config = dynamicConfigFile;
	}

	public abstract void Load();

	public abstract void Save();

	public abstract void Create(ChaosPlugin chaosPlugin);
}
