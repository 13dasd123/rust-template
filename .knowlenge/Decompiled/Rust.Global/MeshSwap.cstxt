using System;
using UnityEngine;

[Serializable]
public struct MeshSwap
{
	public Mesh Source;

	public Mesh Dest;

	internal static Mesh GetDest(MeshSwap[] swaps, Mesh sharedMesh)
	{
		for (int i = 0; i < swaps.Length; i++)
		{
			if (sharedMesh == swaps[i].Source)
			{
				return swaps[i].Dest;
			}
		}
		return null;
	}
}
