using System.Collections.Generic;
using Rust;
using UnityEngine;

public static class Entity
{
	private static Dictionary<GameObject, IEntity> _dict = new Dictionary<GameObject, IEntity>();

	public static void Register(GameObject obj, IEntity entity)
	{
		_dict[obj] = entity;
	}

	public static IEntity Get(GameObject obj)
	{
		if (_dict.TryGetValue(obj, out var value))
		{
			return value;
		}
		return null;
	}

	public static void Unregister(GameObject obj)
	{
		_dict.Remove(obj);
	}
}
