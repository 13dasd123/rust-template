	public static void SerializeDelta(Stream stream, AppCameraFrame instance, AppCameraFrame previous)
	{
		MemoryStream obj = Pool.Get<MemoryStream>();
		if (instance.frame != previous.frame)
		{
			stream.WriteByte(8);
			ProtocolParser.WriteUInt32(stream, instance.frame);
		}
		if (instance.jpgImage.Array == null)
		{
			throw new ArgumentNullException("jpgImage", "Required by proto specification.");
		}
		stream.WriteByte(18);
		ProtocolParser.WritePooledBytes(stream, instance.jpgImage);
		Pool.FreeMemoryStream(ref obj);
	}

	public static void Serialize(Stream stream, AppCameraFrame instance)
	{
		MemoryStream obj = Pool.Get<MemoryStream>();
		stream.WriteByte(8);
		ProtocolParser.WriteUInt32(stream, instance.frame);
		if (instance.jpgImage.Array == null)
		{
			throw new ArgumentNullException("jpgImage", "Required by proto specification.");
