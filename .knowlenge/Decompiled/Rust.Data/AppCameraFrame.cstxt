using System;
using System.Buffers;
using System.IO;
using Facepunch;
using ProtoBuf;
using SilentOrbit.ProtocolBuffers;

public class AppCameraFrame : IDisposable, Pool.IPooled, IProto
{
	public bool ShouldPool = true;

	private bool _disposed;

	[NonSerialized]
	public uint frame;

	[NonSerialized]
	public ArraySegment<byte> jpgImage;

	public static void ResetToPool(AppCameraFrame instance)
	{
		if (instance.ShouldPool)
		{
			instance.frame = 0u;
			if (instance.jpgImage.Array != null)
			{
				ArrayPool<byte>.Shared.Return(instance.jpgImage.Array);
			}
			instance.jpgImage = default(ArraySegment<byte>);
			Pool.Free(ref instance);
		}
	}

	public void ResetToPool()
	{
		ResetToPool(this);
	}

	public virtual void Dispose()
	{
		if (!ShouldPool)
		{
			throw new Exception("Trying to dispose AppCameraFrame with ShouldPool set to false!");
		}
		if (!_disposed)
		{
			ResetToPool();
			_disposed = true;
		}
	}

	public virtual void EnterPool()
	{
		_disposed = true;
	}

	public virtual void LeavePool()
	{
		_disposed = false;
	}

	public void CopyTo(AppCameraFrame instance)
	{
		instance.frame = frame;
		if (jpgImage.Array == null)
		{
			instance.jpgImage = default(ArraySegment<byte>);
			return;
		}
		byte[] array = ArrayPool<byte>.Shared.Rent(jpgImage.Count);
		Array.Copy(jpgImage.Array, 0, array, 0, jpgImage.Count);
		instance.jpgImage = new ArraySegment<byte>(array, 0, jpgImage.Count);
	}

	public AppCameraFrame Copy()
	{
		AppCameraFrame appCameraFrame = Pool.Get<AppCameraFrame>();
		CopyTo(appCameraFrame);
		return appCameraFrame;
	}

	public static AppCameraFrame Deserialize(Stream stream)
	{
		AppCameraFrame appCameraFrame = Pool.Get<AppCameraFrame>();
		Deserialize(stream, appCameraFrame, isDelta: false);
		return appCameraFrame;
	}

	public static AppCameraFrame DeserializeLengthDelimited(Stream stream)
	{
		AppCameraFrame appCameraFrame = Pool.Get<AppCameraFrame>();
		DeserializeLengthDelimited(stream, appCameraFrame, isDelta: false);
		return appCameraFrame;
	}

	public static AppCameraFrame DeserializeLength(Stream stream, int length)
	{
		AppCameraFrame appCameraFrame = Pool.Get<AppCameraFrame>();
		DeserializeLength(stream, length, appCameraFrame, isDelta: false);
		return appCameraFrame;
	}

	public static AppCameraFrame Deserialize(byte[] buffer)
	{
		AppCameraFrame appCameraFrame = Pool.Get<AppCameraFrame>();
		using MemoryStream stream = new MemoryStream(buffer);
		Deserialize(stream, appCameraFrame, isDelta: false);
		return appCameraFrame;
	}

	public void FromProto(Stream stream, bool isDelta = false)
