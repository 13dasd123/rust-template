			}
			if (stream.Position != num)
			{
				throw new ProtocolBufferException("Read past max limit");
			}
			return instance;
		}

		public static void SerializeDelta(Stream stream, Private instance, Private previous)
		{
			MemoryStream obj = Pool.Get<MemoryStream>();
			if (instance.deployerID != previous.deployerID)
			{
				stream.WriteByte(8);
				ProtocolParser.WriteUInt64(stream, instance.deployerID);
			}
			if (instance.deployerName != null && instance.deployerName != previous.deployerName)
			{
				stream.WriteByte(18);
				ProtocolParser.WriteString(stream, instance.deployerName);
			}
			Pool.FreeMemoryStream(ref obj);
		}

		public static void Serialize(Stream stream, Private instance)
		{
			MemoryStream obj = Pool.Get<MemoryStream>();
			stream.WriteByte(8);
			ProtocolParser.WriteUInt64(stream, instance.deployerID);
			if (instance.deployerName != null)
			{
				stream.WriteByte(18);
				ProtocolParser.WriteString(stream, instance.deployerName);
			}
			Pool.FreeMemoryStream(ref obj);
		}

