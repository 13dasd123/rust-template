	public static void SerializeDelta(Stream stream, ModularVehicle instance, ModularVehicle previous)
	{
		MemoryStream obj = Pool.Get<MemoryStream>();
		if (instance.moduleContainer != null)
		{
			stream.WriteByte(10);
			obj.SetLength(0L);
			ItemContainer.SerializeDelta(obj, instance.moduleContainer, previous.moduleContainer);
			uint num = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num);
			stream.Write(obj.GetBuffer(), 0, (int)num);
		}
		if (instance.chassisContainer != null)
		{
			stream.WriteByte(18);
			obj.SetLength(0L);
			ItemContainer.SerializeDelta(obj, instance.chassisContainer, previous.chassisContainer);
			uint num2 = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num2);
			stream.Write(obj.GetBuffer(), 0, (int)num2);
		}
		stream.WriteByte(24);
		ProtocolParser.WriteBool(stream, instance.editable);
		Pool.FreeMemoryStream(ref obj);
	}

	public static void Serialize(Stream stream, ModularVehicle instance)
	{
		MemoryStream obj = Pool.Get<MemoryStream>();
		if (instance.moduleContainer != null)
		{
			stream.WriteByte(10);
			obj.SetLength(0L);
			ItemContainer.Serialize(obj, instance.moduleContainer);
			uint num = (uint)obj.Length;
			ProtocolParser.WriteUInt32(stream, num);
