	public static void ResetToPool(ModularVehicle instance)
	{
		if (instance.ShouldPool)
		{
			if (instance.moduleContainer != null)
			{
				instance.moduleContainer.ResetToPool();
				instance.moduleContainer = null;
			}
			if (instance.chassisContainer != null)
			{
				instance.chassisContainer.ResetToPool();
				instance.chassisContainer = null;
			}
			instance.editable = false;
			Pool.Free(ref instance);
		}
	}

	public void ResetToPool()
	{
		ResetToPool(this);
	}

	public virtual void Dispose()
	{
		if (!ShouldPool)
		{
			throw new Exception("Trying to dispose ModularVehicle with ShouldPool set to false!");
		}
		if (!_disposed)
		{
			ResetToPool();
			_disposed = true;
		}
	}

	public virtual void EnterPool()
	{
		_disposed = true;
	}

	public virtual void LeavePool()
	{
		_disposed = false;
	}

	public void CopyTo(ModularVehicle instance)
	{
		if (moduleContainer != null)
		{
			if (instance.moduleContainer == null)
			{
				instance.moduleContainer = moduleContainer.Copy();
			}
			else
			{
				moduleContainer.CopyTo(instance.moduleContainer);
			}
		}
		else
		{
			instance.moduleContainer = null;
		}
		if (chassisContainer != null)
		{
			if (instance.chassisContainer == null)
			{
				instance.chassisContainer = chassisContainer.Copy();
			}
			else
			{
				chassisContainer.CopyTo(instance.chassisContainer);
			}
		}
		else
		{
			instance.chassisContainer = null;
		}
		instance.editable = editable;
	}

	public ModularVehicle Copy()
	{
		ModularVehicle modularVehicle = Pool.Get<ModularVehicle>();
		CopyTo(modularVehicle);
		return modularVehicle;
	}

