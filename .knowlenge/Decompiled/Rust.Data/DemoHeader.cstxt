	}

	public static DemoHeader DeserializeLength(Stream stream, int length, DemoHeader instance, bool isDelta)
	{
		if (!isDelta)
		{
		}
		long num = stream.Position + length;
		while (stream.Position < num)
		{
			int num2 = stream.ReadByte();
			switch (num2)
			{
