using Oxide.Ext.Discord.Builders;
using Oxide.Ext.Discord.Interfaces;

public class InviteLookup : IDiscordQueryString
{
	public bool? WithCounts { get; set; }

	public bool? WithExpiration { get; set; }

	public bool? GuildScheduledEventId { get; set; }

	public string ToQueryString()
	{
		QueryStringBuilder queryStringBuilder = default(QueryStringBuilder);
		if (WithCounts.HasValue)
		{
			queryStringBuilder.Add("with_counts", WithCounts.Value.ToString());
		}
		if (WithExpiration.HasValue)
		{
			queryStringBuilder.Add("with_expiration", WithExpiration.Value.ToString());
		}
		if (GuildScheduledEventId.HasValue)
		{
			queryStringBuilder.Add("guild_scheduled_event_id", GuildScheduledEventId.Value.ToString());
		}
		return queryStringBuilder.ToString();
	}
}
