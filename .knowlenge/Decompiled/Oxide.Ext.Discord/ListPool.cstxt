using System.Collections.Generic;
using Oxide.Ext.Discord.Types;

internal class ListPool<T> : BasePool<List<T>, ListPool<T>>
{
	protected override PoolSize GetPoolSize(PoolSettings settings)
	{
		return settings.ListPoolSize;
	}

	protected override List<T> CreateNew()
	{
		return new List<T>();
	}

	protected override bool OnFreeItem(ref List<T> item)
	{
		item.Clear();
		return true;
	}
}
