using System;
using Oxide.Ext.Discord.Libraries;

public readonly struct PlaceholderDataKey : IEquatable<PlaceholderDataKey>
{
	public readonly string Key;

	public bool IsValid => !string.IsNullOrEmpty(Key);

	public PlaceholderDataKey(string key)
	{
		Key = key;
	}

	public bool Equals(PlaceholderDataKey other)
	{
		return Key == other.Key;
	}

	public override bool Equals(object obj)
	{
		return obj is PlaceholderDataKey other && Equals(other);
	}

	public override int GetHashCode()
	{
		return (Key != null) ? Key.GetHashCode() : 0;
	}
}
