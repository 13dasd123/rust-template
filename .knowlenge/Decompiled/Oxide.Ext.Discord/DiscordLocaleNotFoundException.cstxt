using Oxide.Ext.Discord.Exceptions;
using Oxide.Ext.Discord.Libraries;

public class DiscordLocaleNotFoundException : BaseDiscordException
{
	private DiscordLocaleNotFoundException(string message)
		: base(message)
	{
	}

	internal static DiscordLocaleNotFoundException NotFound(ServerLocale id)
	{
		return new DiscordLocaleNotFoundException("Failed to find discord locale for server locale '" + id.Id + "'");
	}

	internal static void ThrowNotFound(ServerLocale id)
	{
		throw NotFound(id);
	}
}
