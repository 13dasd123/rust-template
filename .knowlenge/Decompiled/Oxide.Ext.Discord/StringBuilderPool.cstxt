using System.Text;
using Oxide.Ext.Discord.Types;

internal class StringBuilderPool : BasePool<StringBuilder, StringBuilderPool>
{
	protected override PoolSize GetPoolSize(PoolSettings settings)
	{
		return settings.StringBuilderPoolSize;
	}

	protected override StringBuilder CreateNew()
	{
		return new StringBuilder();
	}

	protected override bool OnFreeItem(ref StringBuilder item)
	{
		item.Clear();
		return true;
	}
}
