using Oxide.Ext.Discord.Builders;
using Oxide.Ext.Discord.Entities;
using Oxide.Ext.Discord.Interfaces;

public class UserGuildsRequest : IDiscordQueryString
{
	public Snowflake? Before { get; set; }

	public Snowflake? After { get; set; }

	public int Limit { get; set; } = 200;

	public virtual string ToQueryString()
	{
		QueryStringBuilder queryStringBuilder = default(QueryStringBuilder);
		queryStringBuilder.Add("limit", Limit.ToString());
		if (Before.HasValue)
		{
			queryStringBuilder.Add("before", Before.ToString());
		}
		if (After.HasValue)
		{
			queryStringBuilder.Add("after", After.ToString());
		}
		return queryStringBuilder.ToString();
	}
}
