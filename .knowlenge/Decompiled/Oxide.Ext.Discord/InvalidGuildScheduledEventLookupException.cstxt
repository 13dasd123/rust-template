using Oxide.Ext.Discord.Exceptions;

public class InvalidGuildScheduledEventLookupException : BaseDiscordException
{
	private InvalidGuildScheduledEventLookupException(string message)
		: base(message)
	{
	}

	internal static void ThrowIfInvalidLimit(int? limit)
	{
		if (limit > 100)
		{
			throw new InvalidGuildScheduledEventLookupException(string.Format("{0}.{1} cannot be greater than {2}", "ScheduledEventUsersLookup", "Limit", 100));
		}
	}
}
