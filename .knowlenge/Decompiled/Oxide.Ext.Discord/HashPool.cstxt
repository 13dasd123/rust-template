using Oxide.Ext.Discord.Types;
using Oxide.Plugins;

internal class HashPool<TKey, TValue> : BasePool<Hash<TKey, TValue>, HashPool<TKey, TValue>>
{
	protected override PoolSize GetPoolSize(PoolSettings settings)
	{
		return settings.HashPoolSize;
	}

	protected override Hash<TKey, TValue> CreateNew()
	{
		return new Hash<TKey, TValue>();
	}

	protected override bool OnFreeItem(ref Hash<TKey, TValue> item)
	{
		item.Clear();
		return true;
	}
}
