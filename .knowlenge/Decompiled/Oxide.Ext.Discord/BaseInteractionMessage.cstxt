using Oxide.Ext.Discord.Entities;
using Oxide.Ext.Discord.Exceptions;

public abstract class BaseInteractionMessage : BaseMessageCreate
{
	protected override void ValidateRequiredFields()
	{
	}

	protected override void ValidateFlags()
	{
		InvalidMessageException.ThrowIfInvalidFlags(base.Flags, MessageFlags.SuppressEmbeds | MessageFlags.Ephemeral | MessageFlags.SuppressNotifications, "Invalid Message Flags Used for Channel Message. Only supported flags are MessageFlags.SuppressEmbeds, MessageFlags.Ephemeral, and MessageFlags.SuppressNotifications");
	}
}
