using Network;

public class Message
{
	public enum Type : byte
	{
		Welcome = 1,
		Auth = 2,
		Approved = 3,
		Ready = 4,
		Entities = 5,
		EntityDestroy = 6,
		GroupChange = 7,
		GroupDestroy = 8,
		RPCMessage = 9,
		EntityPosition = 10,
		ConsoleMessage = 11,
		ConsoleCommand = 12,
		Effect = 13,
		DisconnectReason = 14,
		Tick = 15,
		Message = 16,
		RequestUserInformation = 17,
		GiveUserInformation = 18,
		GroupEnter = 19,
		GroupLeave = 20,
		VoiceData = 21,
		EAC = 22,
		EntityFlags = 23,
		World = 24,
		ConsoleReplicatedVars = 25,
		Last = 25
	}

	public Type type;

	public BaseNetwork peer;

	public Connection connection;

	public NetRead read => peer.read;

	public NetWrite write => peer.write;

	public virtual void Clear()
	{
		connection = null;
		peer = null;
		type = (Type)0;
	}
}
