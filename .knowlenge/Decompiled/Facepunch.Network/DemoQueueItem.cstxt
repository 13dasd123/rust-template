using Network;

public struct DemoQueueItem
{
	public NetWrite write;

	public NetRead read;

	public bool IgnoreNoConnections;

	public DemoQueueItem(NetWrite write)
	{
		this.write = write;
		read = null;
		IgnoreNoConnections = false;
	}

	public DemoQueueItem(NetRead read)
	{
		write = null;
		this.read = read;
		IgnoreNoConnections = false;
	}
}
