using System.Collections.Generic;

public class PlaceBridgeObjects : ProceduralComponent
{
	public PathList.BridgeObject[] Bridge;

	public override void Process(uint seed)
	{
		List<PathList> bridges = TerrainMeta.Path.Bridges;
		foreach (PathList item in bridges)
		{
			PathList.BridgeObject[] bridge = Bridge;
			foreach (PathList.BridgeObject obj in bridge)
			{
				item.SpawnBridge(ref seed, obj);
			}
		}
	}
}
