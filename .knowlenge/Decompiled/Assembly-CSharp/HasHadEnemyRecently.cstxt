using Rust.Ai;
using UnityEngine;

public class HasHadEnemyRecently : BaseScorer
{
	public override float GetScore(BaseContext ctx)
	{
		if (ctx is NPCHumanContext nPCHumanContext && Time.time - nPCHumanContext.Human.LastHasEnemyTime < nPCHumanContext.Human.Stats.AttackedMemoryTime)
		{
			return 1f;
		}
		return 0f;
	}
}
