using Rust.Ai;
using UnityEngine;

public class HasHadEnemyRecently : BaseScorer
{
	public override float GetScore(BaseContext ctx)
	{
		if (ctx is NPCHumanContext nPCHumanContext)
		{
			float num = Time.time - nPCHumanContext.Human.LastHasEnemyTime;
			if (num < nPCHumanContext.Human.Stats.AttackedMemoryTime)
			{
				return 1f;
			}
		}
		return 0f;
	}
}
