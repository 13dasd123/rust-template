using UnityEngine;

public class TerrainCollisionTrigger : EnvironmentVolumeTrigger
{
	protected void OnTriggerEnter(Collider other)
	{
		if ((bool)TerrainMeta.Collision && !other.isTrigger)
		{
			TerrainMeta.Collision.SetIgnore(other, base.volume.trigger);
		}
	}

	protected void OnTriggerExit(Collider other)
	{
		if ((bool)TerrainMeta.Collision && !other.isTrigger)
		{
			TerrainMeta.Collision.SetIgnore(other, base.volume.trigger, ignore: false);
		}
	}
}
