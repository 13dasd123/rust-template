using Apex.AI;

public class AStarIsNavigationBlocked : ContextualScorerBase<ScientistAStarContext>
{
	public override float Score(ScientistAStarContext c)
	{
		if (!CanNavigate(c))
		{
			return score;
		}
		return 0f;
	}

	public static bool CanNavigate(ScientistAStarContext c)
	{
		return false;
	}
}
