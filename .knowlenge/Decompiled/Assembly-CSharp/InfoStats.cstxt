using System;

[Serializable]
public class InfoStats
{
	public Family Family;

	public Family[] Predators;

	public Family[] Prey;

	public BaseNpc.AiStatistics.FamilyEnum ToFamily(Family family)
	{
		return family switch
		{
			Family.Bear => BaseNpc.AiStatistics.FamilyEnum.Bear, 
			Family.Boar => BaseNpc.AiStatistics.FamilyEnum.Boar, 
			Family.Chicken => BaseNpc.AiStatistics.FamilyEnum.Chicken, 
			Family.Deer => BaseNpc.AiStatistics.FamilyEnum.Deer, 
			Family.Horse => BaseNpc.AiStatistics.FamilyEnum.Horse, 
			Family.Wolf => BaseNpc.AiStatistics.FamilyEnum.Wolf, 
			Family.Scientist => BaseNpc.AiStatistics.FamilyEnum.Scientist, 
			Family.Murderer => BaseNpc.AiStatistics.FamilyEnum.Murderer, 
			Family.Zombie => BaseNpc.AiStatistics.FamilyEnum.Zombie, 
			_ => BaseNpc.AiStatistics.FamilyEnum.Player, 
		};
	}
}
