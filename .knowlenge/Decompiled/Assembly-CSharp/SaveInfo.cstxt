using Network;
using ProtoBuf;

public struct SaveInfo
{
	public Entity msg;

	public bool forDisk;

	public Connection forConnection;

	internal bool SendingTo(Connection ownerConnection)
	{
		if (ownerConnection == null)
		{
			return false;
		}
		if (forConnection == null)
		{
			return false;
		}
		return forConnection == ownerConnection;
	}
}
