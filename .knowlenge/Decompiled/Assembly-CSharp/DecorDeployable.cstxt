public class DecorDeployable : BaseCombatEntity
{
	public override bool CanPickup(BasePlayer player)
	{
		return base.CanPickup(player) && player.CanBuild() && (bool)player.GetHeldEntity() && player.GetHeldEntity().GetComponent<Hammer>() != null;
	}

	public override bool SupportsPooling()
	{
		return true;
	}
}
