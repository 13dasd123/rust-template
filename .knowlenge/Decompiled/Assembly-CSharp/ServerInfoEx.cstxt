using System.Linq;
using Steamworks.Data;

internal struct ServerInfoEx
{
	public ServerInfo info;

	public string[] Tags;

	public int Born;

	public ServerInfoEx(ServerInfo server)
	{
		info = server;
		Tags = info.Tags;
		Born = 0;
		string text = Tags.FirstOrDefault((string x) => x.StartsWith("born"));
		if (text != null)
		{
			int.TryParse(text.Substring(4), out Born);
		}
	}
}
