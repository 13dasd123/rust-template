using UnityEngine;

public class ModelConditionTest_Sphere : ModelConditionTest
{
	public float findRadius = 0.1f;

	public bool wantEmpty;

	private void OnDrawGizmos()
	{
		Gizmos.color = ((!wantEmpty) ? Color.red : Color.green);
		Gizmos.DrawSphere(base.transform.position, findRadius);
	}

	public override bool DoTest(GameObject obj)
	{
		return false;
	}
}
