using Apex.AI;
using Apex.Serialization;

public class AStarHasWorldStateGreaterThan : ContextualScorerBase<ScientistAStarContext>
{
	[ApexSerialization]
	public Facts Fact;

	[ApexSerialization]
	public byte Value;

	public override float Score(ScientistAStarContext c)
	{
		if (c.GetWorldState(Fact) <= Value)
		{
			return 0f;
		}
		return score;
	}
}
