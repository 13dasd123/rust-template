using Apex.AI;
using Apex.Serialization;

public class AStarHasWorldStateLessThan : ContextualScorerBase<ScientistAStarContext>
{
	[ApexSerialization]
	public Facts Fact;

	[ApexSerialization]
	public byte Value;

	public override float Score(ScientistAStarContext c)
	{
		return (c.GetWorldState(Fact) >= Value) ? 0f : score;
	}
}
