using Apex.AI;
using UnityEngine;

public class MurdererCanNavigateCloserToPrimaryPlayerTarget : ContextualScorerBase<MurdererContext>
{
	public override float Score(MurdererContext c)
	{
		return (!Try(c)) ? 0f : score;
	}

	public static bool Try(MurdererContext c)
	{
		Vector3 destination = MurdererNavigateCloserToPrimaryPlayerTarget.GetDestination(c);
		float num = (destination - c.BodyPosition).SqrMagnitudeXZ();
		if (num < 1f)
		{
			return false;
		}
		return true;
	}
}
