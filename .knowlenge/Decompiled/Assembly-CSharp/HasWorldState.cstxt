using Apex.AI;
using Apex.Serialization;

public class HasWorldState : ContextualScorerBase<ScientistContext>
{
	[ApexSerialization]
	public Facts Fact;

	[ApexSerialization]
	public byte Value;

	public override float Score(ScientistContext c)
	{
		return (c.GetWorldState(Fact) != Value) ? 0f : score;
	}
}
