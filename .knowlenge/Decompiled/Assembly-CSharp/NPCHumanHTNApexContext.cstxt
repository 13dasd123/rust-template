using System.Collections.Generic;
using Rust.Ai.HTN;

public class NPCHumanHTNApexContext : BaseNPCContext, IHTNContext
{
	public NPCPlayerHTNApex Human { get; private set; }

	public PlanStateType PlanState { get; set; }

	public List<PrimitiveTaskSelector> HtnPlan { get; private set; }

	public NPCHumanHTNApexContext(NPCPlayerHTNApex human)
		: base(human)
	{
		Human = human;
		PlanState = PlanStateType.Complete;
		HtnPlan = new List<PrimitiveTaskSelector>(2);
	}
}
