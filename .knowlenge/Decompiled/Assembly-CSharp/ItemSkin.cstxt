using Rust.Workshop;
using UnityEngine;

public class ItemSkin : SteamInventoryItem
{
	public Skinnable Skinnable;

	public Material[] Materials;

	public MeshSwap[] Meshes;

	public void ApplySkin(GameObject obj)
	{
		if (!(Skinnable == null))
		{
			Skin.Apply(obj, Skinnable, Materials, Meshes);
		}
	}
}
