using Network;
using UnityEngine;
using UnityEngine.UI;

public class NetworkInfoGeneralText : MonoBehaviour
{
	public Text text;

	private void Update()
	{
		UpdateText();
	}

	private void UpdateText()
	{
		string text = string.Empty;
		if (Net.sv != null)
		{
			text += "Server\n";
			text += Net.sv.GetDebug(null);
			text += "\n";
		}
		this.text.text = text;
	}

	private static string ChannelStat(int window, int left)
	{
		return $"{left}/{window}";
	}
}
