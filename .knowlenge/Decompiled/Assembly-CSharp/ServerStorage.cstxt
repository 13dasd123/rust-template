using System.IO;
using ConVar;

public class ServerStorage : FileStorage
{
	public override string GetFolder(uint entityid, uint numid, bool create)
	{
		string text = $"{Server.rootFolder}/storage/{entityid}/{numid}/";
		if (create && !Directory.Exists(text))
		{
			Directory.CreateDirectory(text);
		}
		return text;
	}

	public override void Cleanup()
	{
	}
}
