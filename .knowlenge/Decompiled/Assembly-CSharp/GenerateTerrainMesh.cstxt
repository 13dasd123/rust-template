public class GenerateTerrainMesh : ProceduralComponent
{
	public override bool RunOnCache => true;

	public override void Process(uint seed)
	{
		if (!World.Serialization.Cached)
		{
			World.Serialization.AddMap("terrain", TerrainMeta.HeightMap.ToByteArray());
		}
		TerrainMeta.HeightMap.ApplyToTerrain();
		if (World.Serialization.Cached)
		{
			TerrainMeta.HeightMap.FromByteArray(World.Serialization.GetMap("height"));
		}
	}
}
