using UnityEngine;
using UnityEngine.Rendering;

[ExecuteInEditMode]
[RequireComponent(typeof(CommandBufferManager))]
public class PostOpaqueDepth : SingletonComponent<PostOpaqueDepth>
{
	private Material copyDepthMat;

	public RenderTexture cameraDepth;

	public RenderTexture postOpaqueDepth;

	private const CameraEvent commandBufferEvent = CameraEvent.BeforeLighting;

	private const int commandBufferOrderId = 200;

	public Material CopyDepthMat => copyDepthMat;

	public RenderTexture PostOpaque => postOpaqueDepth;
}
