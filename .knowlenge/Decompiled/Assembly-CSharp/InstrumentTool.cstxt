#define UNITY_ASSERTIONS
using System;
using ConVar;
using Network;
using UnityEngine;
using UnityEngine.Assertions;

public class InstrumentTool : HeldEntity
{
	public GameObjectRef[] soundEffect = new GameObjectRef[2];

	public override bool OnRpcMessage(BasePlayer player, uint rpc, Message msg)
	{
		using (TimeWarning.New("InstrumentTool.OnRpcMessage"))
		{
			if (rpc == 3752805966u && player != null)
			{
				Assert.IsTrue(player.isServer, "SV_RPC Message is using a clientside player!");
				if (Global.developer > 2)
				{
					Debug.Log(string.Concat("SV_RPCMessage: ", player, " - SVPlayNote "));
				}
				using (TimeWarning.New("SVPlayNote"))
				{
					using (TimeWarning.New("Conditions"))
					{
						if (!RPC_Server.IsActiveItem.Test("SVPlayNote", this, player))
						{
							return true;
						}
					}
					try
					{
						using (TimeWarning.New("Call"))
						{
							RPCMessage rPCMessage = default(RPCMessage);
							rPCMessage.connection = msg.connection;
							rPCMessage.player = player;
							rPCMessage.read = msg.read;
							RPCMessage msg2 = rPCMessage;
							SVPlayNote(msg2);
						}
					}
					catch (Exception exception)
					{
						player.Kick("RPC Error in SVPlayNote");
						Debug.LogException(exception);
					}
				}
				return true;
			}
		}
		return base.OnRpcMessage(player, rpc, msg);
	}

	[RPC_Server.IsActiveItem]
	[RPC_Server]
	private void SVPlayNote(RPCMessage msg)
	{
		byte b = msg.read.UInt8();
		float scale = msg.read.Float();
		EffectNetwork.Send(new Effect(soundEffect[b].resourcePath, this, 0u, Vector3.zero, Vector3.forward, msg.connection)
		{
			scale = scale
		});
	}
}
