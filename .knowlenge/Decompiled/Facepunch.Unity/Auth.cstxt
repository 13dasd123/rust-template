using System;
using Facepunch.Models;

public class Auth
{
	public string Type;

	public string Id;

	public string Ticket;

	public string Name;

	public static Auth Steam(byte[] ticket, ulong steamId, string username)
	{
		Auth auth = new Auth();
		auth.Type = "steam";
		auth.Name = username;
		auth.Id = steamId.ToString();
		auth.Ticket = BitConverter.ToString(ticket).Replace("-", string.Empty);
		return auth;
	}
}
