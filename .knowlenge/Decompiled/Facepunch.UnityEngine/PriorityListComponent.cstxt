using UnityEngine;

public class PriorityListComponent<T> : ListComponent<T> where T : MonoBehaviour
{
	public static T Instance
	{
		get
		{
			int count = ListComponent<T>.InstanceList.Count;
			if (count == 0)
			{
				return null;
			}
			return ListComponent<T>.InstanceList[count - 1];
		}
	}
}
