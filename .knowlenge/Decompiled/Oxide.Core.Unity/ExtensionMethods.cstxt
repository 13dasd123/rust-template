using System;
using System.Linq;
using UnityEngine;

public static class ExtensionMethods
{
	public static Vector3 ToVector3(this string vector3)
	{
		float[] array = vector3.Split(',').Select(Convert.ToSingle).ToArray();
		if (array.Length != 3)
		{
			return Vector3.zero;
		}
		return new Vector3(array[0], array[1], array[2]);
	}
}
