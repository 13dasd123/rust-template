using SteamNative;

internal struct PackageId_t
{
	public uint Value;

	public static implicit operator PackageId_t(uint value)
	{
		PackageId_t result = default(PackageId_t);
		result.Value = value;
		return result;
	}

	public static implicit operator uint(PackageId_t value)
	{
		return value.Value;
	}
}
