using System;
using SteamNative;

internal struct BREAKPAD_HANDLE
{
	public IntPtr Value;

	public static implicit operator BREAKPAD_HANDLE(IntPtr value)
	{
		BREAKPAD_HANDLE result = default(BREAKPAD_HANDLE);
		result.Value = value;
		return result;
	}

	public static implicit operator IntPtr(BREAKPAD_HANDLE value)
	{
		return value.Value;
	}
}
