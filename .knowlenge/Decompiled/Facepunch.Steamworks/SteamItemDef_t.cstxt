using SteamNative;

internal struct SteamItemDef_t
{
	public int Value;

	public static implicit operator SteamItemDef_t(int value)
	{
		SteamItemDef_t result = default(SteamItemDef_t);
		result.Value = value;
		return result;
	}

	public static implicit operator int(SteamItemDef_t value)
	{
		return value.Value;
	}
}
