using SteamNative;

internal struct HAuthTicket
{
	public uint Value;

	public static implicit operator HAuthTicket(uint value)
	{
		HAuthTicket result = default(HAuthTicket);
		result.Value = value;
		return result;
	}

	public static implicit operator uint(HAuthTicket value)
	{
		return value.Value;
	}
}
