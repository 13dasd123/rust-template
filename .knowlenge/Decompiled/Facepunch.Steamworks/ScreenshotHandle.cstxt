using SteamNative;

internal struct ScreenshotHandle
{
	public uint Value;

	public static implicit operator ScreenshotHandle(uint value)
	{
		ScreenshotHandle result = default(ScreenshotHandle);
		result.Value = value;
		return result;
	}

	public static implicit operator uint(ScreenshotHandle value)
	{
		return value.Value;
	}
}
