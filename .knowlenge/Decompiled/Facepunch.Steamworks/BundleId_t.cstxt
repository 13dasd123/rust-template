using SteamNative;

internal struct BundleId_t
{
	public uint Value;

	public static implicit operator BundleId_t(uint value)
	{
		BundleId_t result = default(BundleId_t);
		result.Value = value;
		return result;
	}

	public static implicit operator uint(BundleId_t value)
	{
		return value.Value;
	}
}
