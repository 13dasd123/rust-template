using SteamNative;

internal struct UGCHandle_t
{
	public ulong Value;

	public static implicit operator UGCHandle_t(ulong value)
	{
		UGCHandle_t result = default(UGCHandle_t);
		result.Value = value;
		return result;
	}

	public static implicit operator ulong(UGCHandle_t value)
	{
		return value.Value;
	}
}
