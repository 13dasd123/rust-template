using SteamNative;

internal struct HSteamPipe
{
	public int Value;

	public static implicit operator HSteamPipe(int value)
	{
		HSteamPipe result = default(HSteamPipe);
		result.Value = value;
		return result;
	}

	public static implicit operator int(HSteamPipe value)
	{
		return value.Value;
	}
}
