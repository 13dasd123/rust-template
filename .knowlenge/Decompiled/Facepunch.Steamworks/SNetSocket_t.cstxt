using SteamNative;

internal struct SNetSocket_t
{
	public uint Value;

	public static implicit operator SNetSocket_t(uint value)
	{
		SNetSocket_t result = default(SNetSocket_t);
		result.Value = value;
		return result;
	}

	public static implicit operator uint(SNetSocket_t value)
	{
		return value.Value;
	}
}
