using SteamNative;

internal struct HSteamUser
{
	public int Value;

	public static implicit operator HSteamUser(int value)
	{
		HSteamUser result = default(HSteamUser);
		result.Value = value;
		return result;
	}

	public static implicit operator int(HSteamUser value)
	{
		return value.Value;
	}
}
