using System;
using System.Runtime.InteropServices;
using SteamNative;

[StructLayout(LayoutKind.Sequential, Pack = 8)]
internal struct LeaderboardEntry_t
{
	[StructLayout(LayoutKind.Sequential, Pack = 4)]
	internal struct PackSmall
	{
		public ulong SteamIDUser;

		public int GlobalRank;

		public int Score;

		public int CDetails;

		public ulong UGC;

		public static implicit operator LeaderboardEntry_t(PackSmall d)
		{
			LeaderboardEntry_t result = default(LeaderboardEntry_t);
			result.SteamIDUser = d.SteamIDUser;
			result.GlobalRank = d.GlobalRank;
			result.Score = d.Score;
			result.CDetails = d.CDetails;
			result.UGC = d.UGC;
			return result;
		}
	}

	public ulong SteamIDUser;

	public int GlobalRank;

	public int Score;

	public int CDetails;

	public ulong UGC;

	public static LeaderboardEntry_t FromPointer(IntPtr p)
	{
		if (Platform.PackSmall)
		{
			return (PackSmall)Marshal.PtrToStructure(p, typeof(PackSmall));
		}
		return (LeaderboardEntry_t)Marshal.PtrToStructure(p, typeof(LeaderboardEntry_t));
	}
}
