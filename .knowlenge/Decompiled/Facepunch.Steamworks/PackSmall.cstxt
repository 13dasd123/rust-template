using System.Runtime.InteropServices;

[StructLayout(LayoutKind.Sequential, Pack = 4)]
internal struct PackSmall
{
	internal byte FailureType;

	public static implicit operator IPCFailure_t(PackSmall d)
	{
		IPCFailure_t result = default(IPCFailure_t);
		result.FailureType = d.FailureType;
		return result;
	}
}
