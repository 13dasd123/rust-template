using SteamNative;

internal struct SteamAPICall_t
{
	public ulong Value;

	public static implicit operator SteamAPICall_t(ulong value)
	{
		SteamAPICall_t result = default(SteamAPICall_t);
		result.Value = value;
		return result;
	}

	public static implicit operator ulong(SteamAPICall_t value)
	{
		return value.Value;
	}
}
