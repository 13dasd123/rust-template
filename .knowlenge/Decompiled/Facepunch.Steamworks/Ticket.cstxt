using System;

public class Ticket : IDisposable
{
	internal Client client;

	public byte[] Data;

	public uint Handle;

	public void Cancel()
	{
		if (client.IsValid && Handle != 0)
		{
			client.native.user.CancelAuthTicket(Handle);
			Handle = 0u;
			Data = null;
		}
	}

	public void Dispose()
	{
		Cancel();
	}
}
