public class Segment
{
	public float offsetX;

	public float offsetY;

	public float scaleX;

	public float scaleY;

	public float lnA;

	public float B;

	public float Eval(float x)
	{
		float num = (x - offsetX) * scaleX;
		float num2 = 0f;
		if (num > 0f)
		{
			num2 = Mathf.Exp(lnA + B * Mathf.Log(num));
		}
		return num2 * scaleY + offsetY;
	}
}
