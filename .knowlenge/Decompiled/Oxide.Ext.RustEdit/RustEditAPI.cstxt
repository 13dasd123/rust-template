using System.Linq;
using Oxide.Ext.RustEdit;
using Oxide.Ext.RustEdit.APC;
using Oxide.Ext.RustEdit.NPC;
using UnityEngine;

public class RustEditAPI
{
	public static string[] GetTopologyMapNames()
	{
		return CustomTopologyLayers.\u206e\u206c\u200d\u206f\u202b\u200c\u202b\u206c\u200f\u206e\u206a\u202c\u202e\u202b\u202e\u206b\u200d\u206d\u200d\u200c\u206d\u206b\u206d\u202a\u206e\u200f\u202d\u200b\u202c\u206a\u206c\u202c\u206a\u202c\u206d\u202b\u200c\u200c\u202e\u202d\u202e.Keys.ToArray();
	}

	public static bool TryGetTopologyMap(string name, out TerrainTopologyMap terrainTopologyMap)
	{
		return CustomTopologyLayers.\u206e\u206c\u200d\u206f\u202b\u200c\u202b\u206c\u200f\u206e\u206a\u202c\u202e\u202b\u202e\u206b\u200d\u206d\u200d\u200c\u206d\u206b\u206d\u202a\u206e\u200f\u202d\u200b\u202c\u206a\u206c\u202c\u206a\u202c\u206d\u202b\u200c\u200c\u202e\u202d\u202e.TryGetValue(name, out terrainTopologyMap);
	}

	public static void GetAllMapEntities(ref List<BaseEntity> list)
	{
		list.AddRange(DeployableCore.\u206e\u206f\u206f\u206f\u202b\u202a\u202d\u206a\u202d\u206d\u200c\u200e\u206e\u206b\u202c\u206e\u200d\u202c\u200c\u200e\u206d\u206f\u200d\u200c\u206f\u202c\u206c\u202d\u202b\u206e\u200e\u206b\u200f\u202b\u206f\u202a\u206d\u206c\u202d\u202a\u202e);
		list.AddRange(IOCore.\u206a\u200b\u202e\u202e\u202b\u206f\u206d\u206a\u206d\u200b\u202c\u206e\u200e\u200c\u200b\u200b\u206b\u206f\u200c\u200c\u202b\u206d\u206c\u202a\u200c\u202b\u202b\u206b\u202c\u202b\u200b\u206c\u206b\u206b\u206d\u202e\u206d\u200f\u206f\u202e\u202e);
		list.AddRange(LootCore.\u200c\u206b\u206a\u206e\u202a\u206b\u206f\u202b\u206a\u200d\u206e\u200e\u206f\u206d\u202c\u200e\u206d\u200f\u206b\u202d\u202a\u200f\u200f\u206a\u206a\u206a\u202a\u202a\u202a\u206d\u202c\u206e\u206f\u202d\u202c\u200f\u202d\u202a\u206d\u206d\u202e.Select((LootCore.\u206c\u202e\u200d\u202b\u206c\u206f\u206d\u202c\u202b\u200c\u206f\u206f\u206d\u206b\u206b\u200e\u202b\u206b\u202a\u206c\u206b\u202e\u206a\u206c\u206a\u206c\u202d\u200c\u202a\u206b\u200f\u200f\u206f\u206d\u202e\u206a\u202a\u200c\u202a\u202e\u202e x) => x.\u200d\u200f\u202b\u200b\u202e\u206b\u202c\u200b\u206d\u200c\u206c\u206d\u206f\u200b\u202d\u200c\u206e\u200c\u202d\u200f\u200c\u202d\u200c\u202d\u206f\u206f\u200f\u202c\u206a\u202c\u200d\u202b\u206f\u202a\u202c\u206b\u200b\u200c\u206c\u202a\u202e));
		list.AddRange(ResourceCore.\u200f\u200e\u202b\u200f\u206e\u202b\u206a\u206e\u200d\u202b\u200b\u200e\u206b\u202d\u200d\u202a\u202b\u206d\u206d\u200f\u202c\u202e\u202d\u200f\u202c\u206f\u206c\u206d\u200f\u202a\u206a\u202a\u206b\u202c\u202b\u200b\u206f\u202a\u200b\u206a\u202e.Select((ResourceCore.\u200d\u202a\u200b\u206c\u200d\u202d\u202d\u206e\u206f\u202b\u200b\u202a\u206c\u206b\u206f\u200e\u200f\u202c\u202e\u206e\u206a\u206d\u200b\u202e\u202e\u206f\u200e\u206e\u202c\u206d\u200f\u202a\u200b\u202e\u206e\u202b\u202c\u206e\u200d\u202d\u202e x) => x.\u200d\u206d\u206f\u200c\u200d\u200c\u206b\u206d\u200d\u206a\u200f\u200d\u202c\u200b\u206f\u206b\u206b\u206f\u200d\u202c\u202a\u206f\u200c\u206e\u200e\u200d\u202a\u206c\u206f\u206e\u200d\u200b\u200c\u200b\u206f\u200e\u202a\u202d\u206f\u202d\u202e));
	}

	public static void GetMapEntitiesOfType<T>(ref List<T> list) where T : BaseEntity
	{
		for (int i = 0; i < DeployableCore.\u206e\u206f\u206f\u206f\u202b\u202a\u202d\u206a\u202d\u206d\u200c\u200e\u206e\u206b\u202c\u206e\u200d\u202c\u200c\u200e\u206d\u206f\u200d\u200c\u206f\u202c\u206c\u202d\u202b\u206e\u200e\u206b\u200f\u202b\u206f\u202a\u206d\u206c\u202d\u202a\u202e.Count; i++)
		{
			T val = DeployableCore.\u206e\u206f\u206f\u206f\u202b\u202a\u202d\u206a\u202d\u206d\u200c\u200e\u206e\u206b\u202c\u206e\u200d\u202c\u200c\u200e\u206d\u206f\u200d\u200c\u206f\u202c\u206c\u202d\u202b\u206e\u200e\u206b\u200f\u202b\u206f\u202a\u206d\u206c\u202d\u202a\u202e[i] as T;
			if (val != null)
			{
				list.Add(val);
			}
		}
		for (int j = 0; j < IOCore.\u206a\u200b\u202e\u202e\u202b\u206f\u206d\u206a\u206d\u200b\u202c\u206e\u200e\u200c\u200b\u200b\u206b\u206f\u200c\u200c\u202b\u206d\u206c\u202a\u200c\u202b\u202b\u206b\u202c\u202b\u200b\u206c\u206b\u206b\u206d\u202e\u206d\u200f\u206f\u202e\u202e.Count; j++)
		{
			T val2 = IOCore.\u206a\u200b\u202e\u202e\u202b\u206f\u206d\u206a\u206d\u200b\u202c\u206e\u200e\u200c\u200b\u200b\u206b\u206f\u200c\u200c\u202b\u206d\u206c\u202a\u200c\u202b\u202b\u206b\u202c\u202b\u200b\u206c\u206b\u206b\u206d\u202e\u206d\u200f\u206f\u202e\u202e[j] as T;
			if (val2 != null)
			{
				list.Add(val2);
			}
		}
		for (int k = 0; k < LootCore.\u200c\u206b\u206a\u206e\u202a\u206b\u206f\u202b\u206a\u200d\u206e\u200e\u206f\u206d\u202c\u200e\u206d\u200f\u206b\u202d\u202a\u200f\u200f\u206a\u206a\u206a\u202a\u202a\u202a\u206d\u202c\u206e\u206f\u202d\u202c\u200f\u202d\u202a\u206d\u206d\u202e.Count; k++)
		{
			T val3 = LootCore.\u200c\u206b\u206a\u206e\u202a\u206b\u206f\u202b\u206a\u200d\u206e\u200e\u206f\u206d\u202c\u200e\u206d\u200f\u206b\u202d\u202a\u200f\u200f\u206a\u206a\u206a\u202a\u202a\u202a\u206d\u202c\u206e\u206f\u202d\u202c\u200f\u202d\u202a\u206d\u206d\u202e[k]?.\u200d\u200f\u202b\u200b\u202e\u206b\u202c\u200b\u206d\u200c\u206c\u206d\u206f\u200b\u202d\u200c\u206e\u200c\u202d\u200f\u200c\u202d\u200c\u202d\u206f\u206f\u200f\u202c\u206a\u202c\u200d\u202b\u206f\u202a\u202c\u206b\u200b\u200c\u206c\u202a\u202e as T;
			if (val3 != null)
			{
				list.Add(val3);
			}
		}
		for (int l = 0; l < VendingCore.\u200f\u206a\u206c\u206f\u202d\u206f\u202e\u200c\u206a\u202b\u206e\u206a\u202b\u200c\u200e\u206d\u202a\u202b\u202d\u202d\u206c\u206b\u202a\u206b\u206a\u202b\u202e\u200c\u206f\u202b\u200b\u200b\u200b\u206d\u206f\u202d\u202c\u202d\u200f\u200d\u202e.Count; l++)
		{
			T val4 = VendingCore.\u200f\u206a\u206c\u206f\u202d\u206f\u202e\u200c\u206a\u202b\u206e\u206a\u202b\u200c\u200e\u206d\u202a\u202b\u202d\u202d\u206c\u206b\u202a\u206b\u206a\u202b\u202e\u200c\u206f\u202b\u200b\u200b\u200b\u206d\u206f\u202d\u202c\u202d\u200f\u200d\u202e[l]?.\u206b\u202b\u200b\u206a\u200c\u206b\u206f\u202b\u202d\u206d\u202a\u202c\u200c\u206f\u202c\u200c\u206f\u206d\u206e\u200b\u206c\u200c\u206c\u206e\u206a\u202c\u200d\u206b\u206e\u202d\u200f\u206d\u200c\u206f\u200e\u206e\u206b\u206e\u202c\u202c\u202e as T;
			if (val4 != null)
			{
				list.Add(val4);
			}
		}
		for (int m = 0; m < ResourceCore.\u200f\u200e\u202b\u200f\u206e\u202b\u206a\u206e\u200d\u202b\u200b\u200e\u206b\u202d\u200d\u202a\u202b\u206d\u206d\u200f\u202c\u202e\u202d\u200f\u202c\u206f\u206c\u206d\u200f\u202a\u206a\u202a\u206b\u202c\u202b\u200b\u206f\u202a\u200b\u206a\u202e.Count; m++)
		{
			T val5 = ResourceCore.\u200f\u200e\u202b\u200f\u206e\u202b\u206a\u206e\u200d\u202b\u200b\u200e\u206b\u202d\u200d\u202a\u202b\u206d\u206d\u200f\u202c\u202e\u202d\u200f\u202c\u206f\u206c\u206d\u200f\u202a\u206a\u202a\u206b\u202c\u202b\u200b\u206f\u202a\u200b\u206a\u202e[m]?.\u200d\u206d\u206f\u200c\u200d\u200c\u206b\u206d\u200d\u206a\u200f\u200d\u202c\u200b\u206f\u206b\u206b\u206f\u200d\u202c\u202a\u206f\u200c\u206e\u200e\u200d\u202a\u206c\u206f\u206e\u200d\u200b\u200c\u200b\u206f\u200e\u202a\u202d\u206f\u202d\u202e as T;
			if (val5 != null)
			{
				list.Add(val5);
			}
		}
	}

	public static void GetActiveNPCs(ref List<BaseCombatEntity> list)
	{
		for (int i = 0; i < NPCCore.\u202d\u202c\u202a\u200e\u206d\u206e\u202d\u202e\u206c\u206f\u200d\u200b\u206b\u206a\u206a\u200b\u206d\u200c\u206c\u200d\u202a\u206c\u206c\u206f\u200f\u206e\u202b\u202b\u200b\u206a\u200d\u202e\u200d\u202b\u206a\u206b\u200d\u200f\u206c\u206a\u202e.Count; i++)
		{
			Oxide.Ext.RustEdit.NPC.NPCSpawner nPCSpawner = NPCCore.\u202d\u202c\u202a\u200e\u206d\u206e\u202d\u202e\u206c\u206f\u200d\u200b\u206b\u206a\u206a\u200b\u206d\u200c\u206c\u200d\u202a\u206c\u206c\u206f\u200f\u206e\u202b\u202b\u200b\u206a\u200d\u202e\u200d\u202b\u206a\u206b\u200d\u200f\u206c\u206a\u202e[i];
			if (nPCSpawner?.\u206a\u200e\u206b\u200b\u200f\u202b\u206d\u206d\u200c\u206c\u202a\u202d\u200e\u202e\u206f\u202c\u200e\u206e\u202a\u200e\u206f\u200e\u200d\u202e\u202c\u202c\u206c\u206c\u200d\u200d\u200d\u200b\u206c\u200e\u206d\u202b\u200b\u206a\u202a\u206f\u202e != null)
			{
				list.Add(nPCSpawner.\u206a\u200e\u206b\u200b\u200f\u202b\u206d\u206d\u200c\u206c\u202a\u202d\u200e\u202e\u206f\u202c\u200e\u206e\u202a\u200e\u206f\u200e\u200d\u202e\u202c\u202c\u206c\u206c\u200d\u200d\u200d\u200b\u206c\u200e\u206d\u202b\u200b\u206a\u202a\u206f\u202e);
			}
		}
	}

	public static void GetActiveAPCs(ref List<BradleyAPC> list)
	{
		for (int i = 0; i < APCCore.\u200d\u202e\u202c\u202e\u202e\u206f\u206e\u200e\u200b\u200e\u206f\u202d\u206f\u202c\u206e\u202d\u200e\u200e\u200e\u206b\u206f\u206a\u206e\u200e\u206c\u202e\u206b\u202c\u206b\u206b\u202b\u200d\u200c\u206b\u202e\u200c\u200d\u200e\u206e\u206f\u202e.Count; i++)
		{
			CustomAPCSpawner customAPCSpawner = APCCore.\u200d\u202e\u202c\u202e\u202e\u206f\u206e\u200e\u200b\u200e\u206f\u202d\u206f\u202c\u206e\u202d\u200e\u200e\u200e\u206b\u206f\u206a\u206e\u200e\u206c\u202e\u206b\u202c\u206b\u206b\u202b\u200d\u200c\u206b\u202e\u200c\u200d\u200e\u206e\u206f\u202e[i];
			if (customAPCSpawner?.\u200e\u200e\u200e\u200b\u206d\u202c\u202a\u202b\u206f\u200b\u200c\u206e\u202c\u202d\u200f\u206b\u206e\u200b\u202e\u206a\u206e\u202a\u200d\u202e\u200c\u206e\u206b\u206a\u202c\u206b\u200c\u206a\u202b\u202d\u202b\u202c\u200b\u206c\u206b\u206d\u202e != null)
			{
				list.Add(customAPCSpawner.\u200e\u200e\u200e\u200b\u206d\u202c\u202a\u202b\u206f\u200b\u200c\u206e\u202c\u202d\u200f\u206b\u206e\u200b\u202e\u206a\u206e\u202a\u200d\u202e\u200c\u206e\u206b\u206a\u202c\u206b\u200c\u206a\u202b\u202d\u202b\u202c\u200b\u206c\u206b\u206d\u202e);
			}
		}
	}

	public static void GetSpawnpoints(ref List<Transform> list)
	{
		list.AddRange(SpawnCore.\u202c\u202b\u206f\u206e\u200f\u202d\u200f\u206d\u200b\u200d\u202e\u200c\u206e\u200b\u202b\u200b\u206e\u206c\u202a\u206c\u200c\u200f\u200e\u200c\u206a\u206f\u206b\u206d\u200b\u206b\u206b\u200b\u206b\u206e\u200f\u200d\u206c\u206b\u200f\u200f\u202e);
	}
