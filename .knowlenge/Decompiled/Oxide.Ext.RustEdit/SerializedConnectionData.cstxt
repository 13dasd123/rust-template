using System;
using ProtoBuf;

[Serializable]
[ProtoContract]
public class SerializedConnectionData
{
	[ProtoMember(1)]
	public string fullPath;

	[ProtoMember(2)]
	public VectorData position;

	[ProtoMember(3)]
	public bool input;

	[ProtoMember(4)]
	public int connectedTo;

	[ProtoMember(5)]
	public int type;
}
